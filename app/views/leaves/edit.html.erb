
<%#= form_with model: @leave, url: edit_user_leave_path, local: true do |form| %>
<div class="field_edit">
  <fieldset>
<%= form_with model: @leave, url: user_leave_path, local: true do |form| %> 
  <p>
    <%= form.label :reason %><br>
    <%= form.text_field :reason %>
  </p>

  <p>
    <%= form.label :leave_type %><br>
    <%= form.text_field :leave_type %>
  </p>

   <p>
    <%= form.label :Start_date %><br>
    <%= form.date_field :start_date %>
  </p>

  <p>
    <%= form.label :end_date %><br>
    <%= form.date_field :end_date %>
  </p>
  <%= form.select :status, ['Pandind', 'Approval', 'Rejected'] %><br />
  <p><br />
    <%= form.submit %>
  </p>
<% end %>
</fieldset>
</div>